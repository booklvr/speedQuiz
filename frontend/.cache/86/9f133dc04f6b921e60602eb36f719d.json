ms].map((team) => {\r\n          if (team.id === payload.id) {\r\n            return {\r\n              ...team,\r\n              name: payload.name,\r\n            }\r\n          }\r\n          return { ...team }\r\n        }),\r\n      }\r\n    case RESET_POINTS:\r\n      return {\r\n        ...state,\r\n        teams: [...state.teams].map((team) => ({\r\n          ...team,\r\n          points: 0,\r\n        })),\r\n      }\r\n    \r\n    default:\r\n      return state\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d3b355ed11301928377184e6520141f0","cacheData":{"env":{}}}