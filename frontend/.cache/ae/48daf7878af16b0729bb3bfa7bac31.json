 {(() => {\n            const time = timeLeft % 60\n            return time.toLocaleString(undefined, { minimumIntegerDigits: 2 })\n          })()}\n        </Col>\n      </Row> */\n}\n"},"lineCount":null}},"error":null,"hash":"197418e9322fb61bcea2298809e8c039","cacheData":{"env":{}}}